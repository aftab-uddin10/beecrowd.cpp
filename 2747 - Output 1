#include <iostream>
 
using namespace std;
 
int main() {
    int i;
    for(i=0;i<39;i++){
 cout<<"-" ;
    }
    cout<<"\n";
    cout<<"|";
    for(i=0;i<37;i++){
        cout<<" " ;
    }
    cout<<"|";
    cout<<"\n";
    cout<<"|";
    for(i=0;i<37;i++){
        cout<<" " ;
    }
    cout<<"|";
    cout<<"\n";
    cout<<"|";
    for(i=0;i<37;i++){
        cout<<" " ;
    }
    cout<<"|";
    cout<<"\n";
    cout<<"|";
    for(i=0;i<37;i++){
        cout<<" " ;
    }
    cout<<"|";
    cout<<"\n";
    cout<<"|";
    for(i=0;i<37;i++){
        cout<<" " ;
    }
    cout<<"|";
    cout<<"\n";
    for(i=0;i<39;i++){
 cout<<"-" ;
    }
    cout<<"\n";
    
    return 0;
}
